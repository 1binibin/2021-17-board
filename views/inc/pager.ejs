<link rel="stylesheet" href="/css/pager.css">
<div class="pager-wrapper">
	<ul class="pager-wrap">
		<li class="pager pager-first">
			<a href="/<%= LANG %>/board/1"class= "material-icons">first_page</a>
		</li>
		<li class="pager pager-prev-set">
			<a href="/<%= LANG %>/board/<%= prevPager%>"class= "material-icons">keyboard_double_arrow_left</a>
		</li>
		<li class="pager pager-prev">
			<a href="/<%= LANG %>/board/<%= prevPage%>"class= "material-icons">chevron_left</a>
		</li>
		<% for(let i=startPage; i<=endPage; i++) { %> 
			<li class="pager <%= page === i ? 'active' : '' %>">
				<a href="/<%= LANG %>/board/<%= i %>"><%= i %></a>
			</li>
			<% } %> 
		</li>
		<li class="pager pager-next">
			<a href="/<%= LANG %>/board/<%= nextPage%>"class= "material-icons">chevron_right</a>
		</li>
		<li class="pager pager-next-set">
			<a href="/<%= LANG %>/board/<%= nextPager%>"class= "material-icons">keyboard_double_arrow_right</a
		</li>
		<li class="pager pager-last">
			<a href="/<%= LANG %>/board/<%= totalPage%>"class= "material-icons">last_page</a>
		</li>
	</ul>
</div>