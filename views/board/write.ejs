<%- include('../inc/head',{ css })%>
<%- include('../inc/header')%>
<section class="write-wrapper wrapper">
	<%- include('../inc/title-bar')%>
	<!-- 본문내용 -->
	<form action="#" method="POST" name="">
		<table class="write-tbl">
			<tbody>
				<tr>
					<td class="title require"><%= FIELD.TITLE%></td>
					<td class="content">
						<input type="text" name="" class="form-control" placeholder="제목">
					</td>
				</tr>
				<tr>
					<td class="title"><%= FIELD.WRITER%></td>
					<td class="content">
						<input type="text" name="" class="form-control" placeholder="작성자">
					</td>
				</tr>
				<tr>
					<td class="title"><%= FIELD.CONTENT%></td>
					<td class="content">
						<textarea name="" class="form-control" placeholder="내용"></textarea>
					</td>
				</tr>
				<tr>
					<td class="title"><%= FIELD.UPFILE%></td>
					<td class="content">
						<input type="file" name="" class="form-control-file">
					</td>
				</tr>
			</tbody>
		</table>
		<div class="btn-wrap p-3 text-center">
			<button class="btn btn-success"><%= BT.CREATE%></button>
			<button class="btn btn-danger" type="reset"><%= BT.RESET%></button>
		</div>
	</form>
</section>
<%- include('../inc/foot.ejs', { js }) %> 