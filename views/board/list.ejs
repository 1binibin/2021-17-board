<%- include('../inc/head',{ css })%>
<%- include('../inc/header')%>
<section class="list-wrapper wrapper">
	<%- include('../inc/title-bar')%>
	<!-- 본문내용 -->
	<table class="board-tbl table table-hover table-dark table-striped text-center">
		<thead>
			<tr>
				<th><%= FIELD.NO%></th>
				<th><%= FIELD.TITLE%></th>
				<th><%= FIELD.WRITER%></th>
				<th><%= FIELD.DATE%></th>
				<th><%= FIELD.READCOUNT%></th>
			</tr>
		</thead>
		<tbody>
			<% for(let board of boards) { %>
			<tr data-idx="<%= board.id %>" data-lang="<%= LANG %>">
				<td><%= board.id %></td>
				<td><%= board.title %></td>
				<td><%= board.writer %></td>
				<td><%= board.createdAt %></td>
				<td><%= board.viewCount %></td>
			</tr>
			<%  } %>
		</tbody>
		</table>
		<%- include('../inc/pager', { ...pager })%>
	</section>
<%- include('../inc/foot', { js })%>